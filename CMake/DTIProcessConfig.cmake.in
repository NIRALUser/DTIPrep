# - Config file for the DTIProcess package
# It defines the following variables
#  DTIProcess_INCLUDE_DIRS - include directories
#  DTIProcess_LIBRARIES    - libraries generated by DTIProcess
#  DTIProcess_EXECUTABLES   - the DTIProcess executable
 
# Compute paths
get_filename_component(DTIProcess_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
set(DTIProcess_INCLUDE_DIRS "@CONF_INCLUDE_DIRS@") 

set(DTIProcess_LIBRARIES 
  cephes
  dtiaverage
  dtiestim
  DTIIO
  dtiprocess
  fiberprocess
  fiberstats
  fibertrack
  maxcurvature
  scalartransform
  TensorOperations
)

find_program(DTIProcess_dtiprocess_EXECUTABLE
  NAMES dtiprocess
  PATHS
    "@CMAKE_INSTALL_PREFIX@/bin"
    "@CMAKE_RUNTIME_OUTPUT_DIRECTORY@"
    "@CMAKE_RUNTIME_OUTPUT_DIRECTORY@/Release"
    "@CMAKE_RUNTIME_OUTPUT_DIRECTORY@/RelWithDebInfo"
    "@CMAKE_RUNTIME_OUTPUT_DIRECTORY@/MinSizeRel"
    "@CMAKE_RUNTIME_OUTPUT_DIRECTORY@/Debug"
  NO_DEFAULT_PATH
  )


find_program(DTIProcess_fiberprocess_EXECUTABLE
  NAMES fiberprocess
  PATHS
    "@CMAKE_INSTALL_PREFIX@/bin"
    "@CMAKE_RUNTIME_OUTPUT_DIRECTORY@"
    "@CMAKE_RUNTIME_OUTPUT_DIRECTORY@/Release"
    "@CMAKE_RUNTIME_OUTPUT_DIRECTORY@/RelWithDebInfo"
    "@CMAKE_RUNTIME_OUTPUT_DIRECTORY@/MinSizeRel"
    "@CMAKE_RUNTIME_OUTPUT_DIRECTORY@/Debug"
  NO_DEFAULT_PATH
  )

